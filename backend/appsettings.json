{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AppConnection": "server=your_server_name;database=your_database_name;uid=your_username;pwd=your_password;Encrypt=False;TrustServerCertificate=true"
  },
  "Jwt": {

    "Key": "your_key", //Secret Key
    "Issuer": "your_issuer", //Authentication Server Domain 	URL Base Address
    "Audience": "your_audience" //Client Application Domain 	URL Base Address
  }
}